  <section id="origin" class="py-20 bg-white">
    <div class="container mx-auto px-4">
      <h2 class="text-3xl md:text-4xl font-bold text-center mb-16 text-neutral-800 animate__animated animate__fadeInUp">Our Story</h2>

      <div class="grid grid-cols-1 md:grid-cols-2 gap-12 items-center">
        <div class="space-y-6 animate__animated animate__fadeInLeft">
          <div class="bg-yellow-50 p-6 rounded-xl shadow-lg">
